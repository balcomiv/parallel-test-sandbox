{
  "name": "parallel-test-sandbox",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "example": "ng test --watch false --browsers ChromeHeadless --include **/*.peer-review.spec.ts",
    "test:administration:pr": "ng test administration --watch false --browsers ChromeHeadless",
    "test:gatling:pr": "ng test gatling --watch false --browsers ChromeHeadless",
    "test:tools:pr": "ng test tools --watch false --browsers ChromeHeadless",
    "test:vendors:pr": "ng test vendors --watch false --browsers ChromeHeadless",
    "cover:lib:tools": "ng test tools --watch false",
    "cover:lib:vendors": "ng test vendors --watch false",
    "cover:app:admin": "ng test administration --watch false",
    "cover:app:gatling": "ng test gatling --watch false",
    "cover": "run-p cover:**",
    "testing:lib:tools:peer-review": "ng test tools --watch false --browsers ChromeHeadless",
    "testing:lib:vendors:peer-review": "ng test vendors --watch false --browsers ChromeHeadless",
    "testing:app:admin:peer-review": "ng test administration --watch false --browsers ChromeHeadless",
    "testing:app:gatling:peer-review": "ng test gatling --watch false --browsers ChromeHeadless",
    "pr": "run-p testing:**:peer-review"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.9",
    "@angular/cdk": "^11.2.0",
    "@angular/common": "~11.0.9",
    "@angular/compiler": "~11.0.9",
    "@angular/core": "~11.0.9",
    "@angular/forms": "~11.0.9",
    "@angular/material": "^11.2.0",
    "@angular/platform-browser": "~11.0.9",
    "@angular/platform-browser-dynamic": "~11.0.9",
    "@angular/router": "~11.0.9",
    "npm-run-all": "^4.1.5",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.7",
    "@angular/cli": "~11.0.7",
    "@angular/compiler-cli": "~11.0.9",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
